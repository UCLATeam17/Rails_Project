<%= javascript_include_tag "JamieAppleseed-selectToAutocomplete/jquery.js" %>
  <%= javascript_include_tag "JamieAppleseed-selectToAutocomplete/jquery-ui-autocomplete.js" %>
  <%= javascript_include_tag "JamieAppleseed-selectToAutocomplete/jquery.select-to-autocomplete.min.js" %>
  <script type="text/javascript" charset="utf-8">
    (function($){
      $(function(){
        $('select').selectToAutocomplete();
      });
    })(jQuery);
  </script>

<div class="  pull-left">
<h2>Sign up</h2>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :title %>
  <%= f.select :title, [['Mr'],['Mrs'],['Ms']] %> </div>

  <div><%= f.label :first_name %>
  <%= f.text_field :first_name %></div>

  <div><%= f.label :last_name %>
  <%= f.text_field :last_name %></div>

  <div><%= f.label :username %> 
  <%= f.text_field :username %> </div>

  <div><%= f.label :email %>
  <%= f.email_field :email %></div>

  <div><%= f.label :country %>
  <%= collection_select :country, :id, Country.all, :id, :name %></div>

  <div><%= f.label :password %>
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up", :class => "btn btn-primary" %></div>
 </div>
<% end %>
</p>

